<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:wm="clr-namespace:Atelier.ViewModel"
                    xmlns:view="clr-namespace:Atelier.View">

    <DataTemplate DataType="{x:Type wm:ClientsVM}">
        <view:Clients />
    </DataTemplate>

    <DataTemplate DataType="{x:Type wm:FittingsVM}">
        <view:Fittings />
    </DataTemplate>

    <DataTemplate DataType="{x:Type wm:ModelsVM}">
        <view:Models />
    </DataTemplate>

    <DataTemplate DataType="{x:Type wm:OrdersVM}">
        <view:Orders />
    </DataTemplate>
</ResourceDictionary>